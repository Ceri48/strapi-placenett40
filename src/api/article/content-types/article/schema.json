{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "regex": "^[a-z0-9]+$"
    },
    "seo": {
      "type": "component",
      "component": "seo.seo-meta",
      "repeatable": false
    }
  }
}
